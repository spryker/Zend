{
  "name": "spryker/zend",
  "type": "library",
  "description": "Zend module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "zendframework/zend-config": "^2.5.1 || ^3.1.0",
    "zendframework/zend-filter": "^2.5.1",
    "zendframework/zend-servicemanager": "^2.7.4 || ^3.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
